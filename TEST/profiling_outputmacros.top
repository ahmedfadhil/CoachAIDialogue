outputmacro: ^set_buttons_on_return_profilation(^health ^coping ^physical ^mental)
	$_responses = ^jsoncreate(array)
	if (^health==0){
		^jsonarrayinsert($_responses "salute")
	}
	if (^coping==0){
		^jsonarrayinsert($_responses "strategia di adattamento")
	}
	if (^physical==0){
		^jsonarrayinsert($_responses "attivita fisica")
	}
	if (^mental==0){
		^jsonarrayinsert($_responses "benessere mentale")
	}
	$USER.buttons = $_responses

outputmacro: ^flag_on_profiling_done(^health ^coping ^physical ^mental)
	$_tmp = 1
	if (^health==0){
		$_tmp = 0
	}else if (^coping==0){
		$_tmp = 0
	}else if (^physical==0){
		$_tmp = 0
	}else if (^mental==0){
		$_tmp = 0
	}
	if($_tmp==1){
		$USER.state=1
	}else{
		$USER.state=0
	}


outputmacro: ^calculate_health_default()
	if ($health==1){
		$health=0
	}else if ($health==3){
		$health=2
	}else if ($health==5){
		$health=4
	}else if ($health==7){
		$health=6
	}else if ($health==9){
		$health=8
	}else if ($health==11){
		$health=10
	}else if ($health=13){
		$health=12
	}else if ($health==15){
		$health=14
	}else if ($health==17){
		$health=16
	}

outputmacro: ^calculate_physical_default()
	if($physical==2 OR $physical==3){
		$physical=1
	}else if($physical==5){
		$physical=4
	}else if($physical==7){
		$physical=6
	}else if($physical==9){
		$physical=8
	}


outputmacro: ^calculate_coping_default()
	if($coping==2){
	    $coping=1
  	}else if($coping==4){
	    $coping=3
  	}else if($coping==6){
	    $coping=5
  	}

outputmacro: ^calculate_mental_default()
  	if ($mental==1){
		$mental=0
	}else if($mental==2){
		$mental=1
	}else if($mental==4){
		$mental=3
	}else if($mental==6){
		$mental=5
	}else if($mental==8){
		$mental=7
	}
