outputmacro: ^upload_health_features()
	$$url = "https://0aa974cc.ngrok.io/webhooks/chatscript_vbc43edbf1614a075954dvd4bfab34l1/upload_health_features"
	$$user_agent = ^"User-Agent: Mozilla/5.0 (compatible; MSIE 10.0; Windows NT 6.2; WOW64; Trident/6.0)"

	$_json_to_send = ^jsoncreate(object)
	^jsonobjectinsert($_json_to_send user_id $USER.user_id)
	^jsonobjectinsert($_json_to_send health_personality $health_personality)
	^jsonobjectinsert($_json_to_send health_wellbeing_meaning $health_wellbeing_meaning)
	^jsonobjectinsert($_json_to_send health_nutritional_habits $health_nutritional_habits)
	^jsonobjectinsert($_json_to_send health_drinking_water $health_drinking_water)
	^jsonobjectinsert($_json_to_send health_vegetables_eaten $health_vegetables_eaten)
	^jsonobjectinsert($_json_to_send health_energy_level $health_energy_level)
	^jsonobjectinsert($_json_to_send notification_hour_morning $notification_hour_morning)
	^jsonobjectinsert($_json_to_send notification_hour_evening $notification_hour_evening)


	$_json_received = ^jsonopen(POST $$url ^jsonwrite($_json_to_send) $$user_agent)
	if (!$_json_received){
		There was a problem during the saving process of the features! Contact the administrator if the problem persists.
		$$tcpopen_error 
	}

